/**app.wxss**/
page {
  font-size: 14px;
  line-height:1.5;
}
.container {
  display: flex; 
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
}
.block {
  position: relative;
  box-shadow: 0 5px 20px rgba(155,155,155,.3);
  border-radius: 4rpx;
}
.block--title {
  position: absolute;
  top: 50rpx;
}
.block .mask {
  overflow: hidden;
  max-height: 300rpx;
}
.mask::after {
  position: absolute;
  content: ' ';
  left: 0;
  top: 40%;
  bottom: 0;
  width: 100%;
  background: linear-gradient(to bottom, rgba(255,255,255, 0), rgba(255, 255, 255, 1));
}
.flex {
  display: flex;
} 
.flex-1 {
  flex: 1;
}
.flex-center-v {
  align-items: center;
}
.flex-bottom-v {
  align-items: flex-end;
}
.f10 {
  font-size: 10px;
}
.f12 {
  font-size: 12px;
}
.f14 {
  font-size: 14px;
}
.f15 {
  font-size: 15px;
}
.f16 {
  font-size: 16px;
}
.f18 {
  font-size: 18px;
}
.f24 {
  font-size: 24px;
}
.fb {
  font-weight: 500;
}
.mb-5 {
  margin-bottom: 10rpx;
}
.mb-10 {
  margin-bottom: 20rpx;
}
.mb-15 {
  margin-bottom: 30rpx;
}
.ml-5 {
  margin-left: 10rpx;
}
.ml-10 {
  margin-left: 20rpx;
}
.mt-20 {
  margin-top: 40rpx;
}
.mt-50 {
  margin-top: 100rpx;
}
.mt-45 {
  margin-top: 90rpx;
}
/* padding */
.pa-25 {
  padding: 50rpx;
}

.text-gray {
  color: rgb(153,153,153);
}
.text-right {
  text-align: right;
}
.ellipsis {
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
  width:100%;
}
.fixed-bottom {
  position: fixed;
  bottom: 60rpx;
  left: 0;
  right: 0;
}
.btn {
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 40rpx;
  border-radius: 4rpx;
  background: linear-gradient(to right, rgb(255, 192, 38), rgb(255, 209, 2));
  box-shadow: 0 5px 10px rgba(252, 147, 61, 0.3);
  text-align: center;
}
.btn--next {
  position: relative;
  margin-left: 75rpx;
  margin-right: 75rpx;
  text-align: left;
}
.btn--next.disabled {
  opacity: .5;
  pointer-events: none;
}
.btn--next:after {
  display: block;
  position: absolute;
  content: ' ';
  height: 50rpx;
  width: 50rpx;
  right: 28rpx;
  top: 22rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 50rpx;
}
.btn--collapse.up {
  transform: rotateZ(180deg);
  top: auto;
  bottom: 30rpx;
}
.btn--collapse {
  transition: transform .25s ease-out;
  position: absolute;
  z-index: 5;
  right: 30rpx;
  top: 30rpx;
  width: 66rpx;
  height: 66rpx;
  background-repeat: no-repeat;
  background-size: 66rpx;
  background-image: url(data:image/png;base64,);
}

.animate-in {
  opacity: 0;
  animation: fade-in .3s ease-out forwards;
  -webkit-animation: fade-in .3s ease-out forwards;
  -moz-animation: fade-in .3s ease-out forwards;
}
.delay-1 {
  animation-delay: 0.3s;
}
.delay-2 {
  animation-delay: 0.5s;
}

@-webkit-keyframes fade-in {
  from {
    opacity: .2;
    transform: translateX(100rpx);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}


@keyframes fade-in {
  from {
    opacity: .2;
    transform: translateX(100rpx);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}
