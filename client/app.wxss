  box-shadow: 0 10rpx 40rpx rgba(155,155,155,.3);
  border-radius: 4rpx;
}
.block--title {
  position: absolute;
  font-weight: bold;
}
.block .mask {
  overflow: hidden;
  position: relative;
  max-height: 185rpx;
}
.mask::after {
  position: absolute;
  content: ' ';
  left: 0;
  top: 0;
  bottom: 0;
  width: 100%;
  background: linear-gradient(to bottom, rgba(255,255,255, 0.25), rgba(255, 255, 255, 1));
}
