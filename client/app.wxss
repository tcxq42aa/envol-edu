.ellipsis {
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
  width:100%;
}
.fixed-bottom {
  position: fixed;
  bottom: 60rpx;
  left: 0;
  right: 0;
}
.tag {
  color: #fff;
  font-size: 24rpx;
  height: 36rpx;
  line-height: 36rpx;
  border-top-left-radius: 18rpx;
  border-bottom-left-radius: 18rpx;
  padding: 0 7rpx 0 16rpx;
}
.tag-blue {
  background: linear-gradient(to right, rgb(51,51,150), rgb(201, 102, 201));
}
.tag-yellow {
  background: linear-gradient(to right, rgb(255, 209, 2), rgb(255, 192, 38));
}
.tag-yellow-reverse {
  background: #fff;
  color: rgb(252, 147, 61);
}
.btn {
  color: #fff;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 40rpx;
  border-radius: 4rpx!important;
  background: linear-gradient(to right, rgb(255, 192, 38), rgb(255, 209, 2));
  box-shadow: 0 10rpx 20rpx rgba(252, 147, 61, 0.3);
  text-align: center;
}
button.btn::after {
  display: none;
}
.btn.disabled {
  opacity: .5; 
   pointer-events: none;
}
.btn-theme-2 {
  background: linear-gradient(to right, rgb(51, 51, 150), rgb(201, 102, 201));
  box-shadow: 0 10rpx 20rpx rgba(201, 102, 201, 0.3);
}
.btn--next {
  position: relative;
  margin-left: 75rpx;
  margin-right: 75rpx;
  text-align: left;
}
.footer-btns {
  display: flex;
  margin-left: 60rpx;
  margin-right: 60rpx;
}
.footer-btns .btn {
  flex-grow: 1;
  flex-basis: 0;
}
.footer-btns .btn + .btn {
  margin-left: 30rpx;
}
.btn--next:after {
  display: block;
  position: absolute;
  content: ' ';
  height: 50rpx;
  width: 50rpx;
  right: 28rpx;
  top: 22rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 50rpx;
}
.btn--collapse.up {
  transform: rotateZ(180deg);
  top: auto;
  bottom: 30rpx;
}
.btn--collapse {
  transition: transform .25s ease-out;
  position: absolute;
  z-index: 5;
  right: 30rpx;
  top: 30rpx;
  width: 66rpx;
  height: 66rpx;
  background-repeat: no-repeat;
  background-size: 66rpx;
  background-image: url(data:image/png;base64,);
}

.animate-in {
  opacity: 0;
  animation: fade-in .3s ease-out forwards;
  -webkit-animation: fade-in .3s ease-out forwards;
  -moz-animation: fade-in .3s ease-out forwards;
}
.delay-1 {
  animation-delay: 0.3s;
}
.delay-2 {
  animation-delay: 0.5s;
}

@-webkit-keyframes fade-in {
  from {
    opacity: .2;
    transform: translateX(100rpx);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}


@keyframes fade-in {
  from {
    opacity: .2;
    transform: translateX(100rpx);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}

.wetoast__bd {
  position: fixed;
  width: 100%;
  bottom: 200rpx;
  opacity: 0;
  text-align: center;
}
.wetoast__bd__title {
  background: rgba(0,0,0,.65);
  color: #fff;
  text-align: left;
  line-height: 1.5;
  padding: 30rpx;
  display: inline-block;
  max-width: 70%;
  border-radius: 10rpx;
}


@font-face {
    font-family: 'DIN';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff');
    font-weight: 300;
    font-style: normal;
}